  "query": "\n            SELECT * FROM tasks\n            WHERE \n                cron_schedule IS NOT NULL\n                AND $1::timestamptz >= next_due_at + (start_window_seconds || ' seconds')::interval + (lateness_window_seconds || ' seconds')::interval\n                AND status = $2 -- status is late\n            LIMIT $3",
