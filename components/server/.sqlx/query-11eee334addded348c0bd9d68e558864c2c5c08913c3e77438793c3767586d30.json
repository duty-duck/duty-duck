  "query": "\n            SELECT *, COUNT(*) OVER () as \"filtered_count!\"\n            FROM task_runs \n            WHERE \n                organization_id = $1\n                AND task_id = $2\n                AND ($3::smallint[] = '{}' OR status = ANY($3))\n            ORDER BY started_at DESC\n            LIMIT $4 OFFSET $5\n            ",
