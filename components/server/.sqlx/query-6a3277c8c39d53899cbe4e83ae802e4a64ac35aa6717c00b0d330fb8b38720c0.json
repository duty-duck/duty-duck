  "db_name": "PostgreSQL",
  "query": "\n            SELECT * FROM tasks\n            WHERE\n                cron_schedule IS NOT NULL\n                AND $1::timestamptz >= next_due_at\n                AND status != $2 -- status is not due \n                AND status != $3 -- status is not running\n                AND status != $4 -- status is not absent\n            LIMIT $5",
