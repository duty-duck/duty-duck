{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT *, COUNT(*) OVER() as \"filtered_count!\" \n            FROM tasks\n            WHERE organization_id = $1\n            AND ($2::integer[] IS NULL OR status = ANY($2))\n            AND (name ILIKE $3 OR description ILIKE $3)\n            ORDER BY name\n            LIMIT $4 OFFSET $5\n            ",
